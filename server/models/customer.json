{
  "name": "Customer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "address": {
      "type": "embedsOne",
      "model": "Address",
      "property": "customerAddress",
      "options": {
        "forceId": false,
        "validate": "true"
      }
    },
    "emails": {
      "type": "embedsMany",
      "model": "EmailAddress",
      "property": "emailList",
      "options": {
        "forceId": false,
        "validate": true
      }
    },
    "orders": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
